{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && npm run clean && webpack --progress -p --config webpack.production.config.js",
    "start": "webpack-dev-server --progress --history-api-fallback --hot --inline --port 9000 --open",
    "dev": "npm run start",
    "dev:remote": "webpack-dev-server --progress --history-api-fallback --hot --inline --host 0.0.0.0 --port 9000 --open",
    "clean": "rimraf ./dist && mkdir dist",
    "lint": "eslint ./app",
    "createServer": "cd dist && npm init -f && npm i express -S"
  },
  "author": "Luke Andersen",
  "license": "ISC",
  "dependencies": {
    "axios": "0.17.0",
    "classnames": "2.2.5",
    "express": "4.16.2",
    "normalize.css": "7.0.0",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-ga": "2.3.5",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "copy-webpack-plugin": "4.2.0",
    "css-loader": "0.28.7",
    "eslint": "4.10.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.30.1",
    "postcss": "6.0.14",
    "postcss-cssnext": "3.0.2",
    "postcss-import": "11.0.0",
    "postcss-load-config": "1.2.0",
    "postcss-loader": "2.0.8",
    "postcss-nesting": "4.2.1",
    "rimraf": "2.6.2",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-cssnext": {
        "browsers": [
          "last 2 versions",
          "> 5%"
        ]
      },
      "postcss-nesting": {}
    }
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "indent": [
        0,
        4
      ],
      "semi": [
        "error",
        "never"
      ],
      "comma-dangle": 0,
      "react/jsx-indent": [
        2,
        4
      ],
      "max-len": [
        1,
        100,
        4,
        {
          "ignorePattern": "\\s*<"
        }
      ],
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": 0,
      "react/require-extension": 0,
      "no-console": 0,
      "react/no-string-refs": 1,
      "no-unused-vars": [
        2,
        {
          "varsIgnorePattern": "[iI]gnored"
        }
      ],
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "react/forbid-prop-types": 0,
      "jsx-a11y/img-has-alt": 0
    }
  }
}
