{
    "name": "react-starter",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "engines": {
        "node": "12.x"
    },
    "scripts": {
        "build": "npm run lint && npm run clean && webpack --progress -p --config webpack.production.config.js",
        "now-build": "npm run build",
        "start": "webpack-dev-server --progress --history-api-fallback --hot --inline --port 9000 --open",
        "dev": "npm run start",
        "dev:remote": "webpack-dev-server --progress --history-api-fallback --hot --inline --host 0.0.0.0 --port 9000 --open",
        "clean": "rimraf ./dist && mkdir dist",
        "lint": "eslint ./app",
        "createServer": "cd dist && npm init -f && npm i express -S"
    },
    "author": "Luke Andersen",
    "license": "ISC",
    "dependencies": {
        "axios": "0.21.1",
        "classnames": "2.2.6",
        "express": "4.17.1",
        "flexboxgrid": "6.3.1",
        "normalize.css": "8.0.1",
        "prop-types": "15.7.2",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-ga": "2.7.0",
        "react-router-dom": "5.1.2"
    },
    "devDependencies": {
        "@babel/core": "7.12.10",
        "@babel/plugin-proposal-class-properties": "7.12.1",
        "@babel/preset-env": "7.12.11",
        "@babel/preset-react": "7.12.10",
        "babel-eslint": "10.1.0",
        "babel-loader": "8.2.2",
        "copy-webpack-plugin": "5.1.2",
        "css-loader": "3.6.0",
        "eslint": "6.8.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-prettier": "6.15.0",
        "eslint-import-resolver-webpack": "0.13.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-prettier": "3.3.1",
        "eslint-plugin-react": "7.22.0",
        "extract-text-webpack-plugin": "4.0.0-beta.0",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "3.2.0",
        "postcss": "7.0.35",
        "postcss-cssnext": "3.1.0",
        "postcss-import": "12.0.1",
        "postcss-load-config": "2.1.2",
        "postcss-loader": "3.0.0",
        "postcss-nesting": "7.0.1",
        "prettier": "1.19.1",
        "react-hot-loader": "4.12.19",
        "rimraf": "3.0.0",
        "style-loader": "1.0.0",
        "url-loader": "3.0.0",
        "webpack": "4.41.5",
        "webpack-cli": "3.3.10",
        "webpack-dev-server": "3.11.0"
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "modules": false
                }
            ],
            "@babel/preset-react"
        ],
        "plugins": [
            "react-hot-loader/babel",
            "@babel/plugin-proposal-class-properties"
        ]
    },
    "prettier": {
        "printWidth": 200,
        "bracketSpacing": true,
        "jsxBracketSameLine": true,
        "trailingComma": "es5",
        "arrowParens": "always",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    },
    "postcss": {
        "plugins": {
            "postcss-import": {},
            "postcss-cssnext": {
                "browsers": [
                    "last 2 versions",
                    "> 5%"
                ]
            },
            "postcss-nesting": {}
        }
    },
    "eslintConfig": {
        "extends": [
            "airbnb",
            "prettier"
        ],
        "parser": "babel-eslint",
        "plugins": [
            "react",
            "jsx-a11y",
            "import",
            "prettier"
        ],
        "env": {
            "es6": true,
            "node": true,
            "browser": true
        },
        "settings": {
            "import/resolver": "webpack"
        },
        "rules": {
            "prettier/prettier": [
                "error"
            ],
            "indent": [
                0,
                4
            ],
            "semi": [
                "error",
                "never"
            ],
            "comma-dangle": 0,
            "react/jsx-indent": [
                2,
                4
            ],
            "react/jsx-indent-props": [
                2,
                4
            ],
            "react/require-default-props": 0,
            "max-len": [
                1,
                100,
                4,
                {
                    "ignorePattern": "\\s*<"
                }
            ],
            "import/no-extraneous-dependencies": 0,
            "import/no-named-as-default": 0,
            "import/no-named-as-default-member": 0,
            "import/no-unresolved": [
                "error",
                {
                    "caseSensitive": false
                }
            ],
            "import/prefer-default-export": 0,
            "react/jsx-filename-extension": 0,
            "react/require-extension": 0,
            "react/jsx-one-expression-per-line": 0,
            "react/state-in-constructor": 0,
            "no-console": 0,
            "react/no-string-refs": 1,
            "no-unused-vars": [
                2,
                {
                    "varsIgnorePattern": "[iI]gnored"
                }
            ],
            "no-underscore-dangle": 0,
            "no-param-reassign": 0,
            "react/forbid-prop-types": 0,
            "jsx-a11y/img-has-alt": 0,
            "jsx-a11y/label-has-for": [
                2,
                {
                    "required": {
                        "every": [
                            "id"
                        ]
                    }
                }
            ],
            "jsx-a11y/label-has-associated-control": [
                2,
                {
                    "assert": "either",
                    "depth": 3
                }
            ],
            "jsx-a11y/anchor-is-valid": [
                "error",
                {
                    "components": [
                        "Link"
                    ],
                    "specialLink": [
                        "to"
                    ]
                }
            ]
        }
    }
}
